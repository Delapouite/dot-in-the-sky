#!/bin/sh
bat --version
broot --version
chars --version
echo "dog $(dog --version | head -n 2 | tail -n 1)"
echo "entr $(entr 2>&1 | head -n 1 | awk '{ print $2 }')"
exa --version
fd --version
echo "fish $(fish --version | awk '{ print $3 }')"
echo "flameshot $(flameshot --version | head -n 1 | awk '{ print $2 }')"
echo "fzf $(fzf --version)"
echo "git $(git --version | awk '{ print $3 }')"
gitui --version
grex --version
echo "htop $(htop --version | head -n 1 | awk '{ print $2 }')"
echo "httpie $(http --version)"
hyperfine --version
echo "i3 $(i3 --version | awk '{ print $3 }')"
jq --version | tr '-' ' '
jwt --version | head -n 1
kak -version | tr '[:upper:]' '[:lower:]'
echo "lazydocker $(lazydocker --version | head -n 1 | awk '{ print $2 }')"
echo "lf $(lf --version)"
meld --version
mons -v | head -n 1 | tr '[:upper:]' '[:lower:]'
echo "mpd $(mpd --version | head -n 1 | awk '{ print $4 }')"
mpv --version | head -n 1 | awk '{ print $1" "$2 }'
ncmpcpp --version | head -n 1 |
nvim --version | head -n 1 | tr '[:upper:]' '[:lower:]'
nu --version
pastel --version
rg --version | head -n 1
echo "rofi $(rofi -v | awk '{ print $2 }')"
rofimoji --version
sad --version
echo "semgrep $(semgrep --version)"
starship --version
sxiv -v
tldr --version
termite --version
tig --version | head -n 1 | awk '{ print $1" "$3 }'
tokei --version | awk '{ print $1" "$2}'
units --version | head -n 1
echo "xsv $(xsv --version)"
zola --version
websocketd --version | awk '{ print $1" "$2 }'
